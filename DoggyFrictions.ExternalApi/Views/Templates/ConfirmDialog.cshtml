<script type="text/html" id="confirm-dialog-template">
    <div class="modal fade confirm-dialog" tabindex="-1" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title" data-bind="text: Title, click: alert"></h4>
                </div>
                <div class="modal-body">
                    <span data-bind="text: Message"></span>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-raised btn-primary" data-dismiss="modal" data-bind="click: Submit">Да</button>
                    <button type="button" class="btn btn-primary" data-dismiss="modal">Нет</button>
                </div>
            </div>
        </div>
    </div>
</script>